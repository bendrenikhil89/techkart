 });\r\n    };\r\n\r\n    function updateCartQuantity(value, cartDetails) {\r\n        let currCart = [...cartContent];\r\n        let currItem = currCart.find(c => {\r\n            return c._id === cartDetails._id;\r\n        });\r\n        if(currItem){\r\n            currItem.count = value;\r\n        }\r\n        setCartContent([...currCart]);\r\n        localStorage.setItem(\"cart\", JSON.stringify(currCart));\r\n        dispatch({\r\n            type: 'ADD_TO_CART',\r\n            payload: currCart\r\n        });\r\n    }\r\n\r\n    function removeProduct(cartDetails){\r\n        let currCart = [...cartContent];\r\n        let currItem = currCart.findIndex(c => {\r\n            return c._id === cartDetails._id;\r\n        });\r\n        if(currItem !== 'undefined' && currItem !== null){\r\n            currCart.splice(currItem, 1);\r\n            setCartContent([...currCart]);\r\n            currCart.length> 0 ? localStorage.setItem(\"cart\", JSON.stringify(currCart)) : localStorage.removeItem(\"cart\");\r\n            dispatch({\r\n                type: 'ADD_TO_CART',\r\n                payload: currCart\r\n            });\r\n        }\r\n    }\r\n\r\n    const productCart = (cartDetails) => {\r\n        return <><div className=\"productDetails__wrapper\">\r\n            <div className=\"productDetails__image\">\r\n                <Image width={100} src={cartDetails && cartDetails.images.length > 0 ? cartDetails.images[0].url : noImage} style={{cursor:'pointer'}} />\r\n            </div>\r\n            <div className=\"productDetails__details\">\r\n                <p>{cartDetails.title}<span style={{paddingLeft:\"20px\"}}>{cartDetails.quantity > 0 ? <Tag color=\"#87d068\">In stock</Tag> : <Tag color=\"red\">Out of stock</Tag> }</span></p>\r\n                <p><strong><CurrencyFormat value={cartDetails.price} displayType={'text'} thousandSeparator={true} prefix={'$'} renderText={value => <span>{value}</span>} /></strong></p>\r\n                <div style={{display:'flex'}}>\r\n                    <div style={{marginRight:'20px',display:'flex',alignItems:'center'}}>\r\n                        <span style={{marginRight:'10px'}}>Quantity:</span><InputNumber min={1} max={cartDetails.quantity} defaultValue={cartDetails.count} onChange={value => updateCartQuantity(value, cartDetails)} />\r\n                    </div>\r\n                    <div  style={{display:'flex',alignItems:'center'}}>\r\n                        <span style={{marginRight:'10px'}}><DeleteFilled style={{color:'#757575',fontSize:'1rem', cursor:'pointer'}} onClick={() => removeProduct(cartDetails)} /></span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <Divider />\r\n        </>\r\n    };\r\n\r\n    const totalCartValue = () => {\r\n        return cart.reduce((total,curr) => total + (curr.count * curr.price), 0);\r\n    }\r\n\r\n    const totalCartProductsCount = () => {\r\n        return cart.reduce((total,curr) => total + curr.count, 0);\r\n    }\r\n\r\n    const checkoutHandler = async(e) => {\r\n        e.preventDefault();\r\n        if(cartContent.length > 0) {\r\n            try{\r\n                await saveCart(email, authtoken, cartContent);\r\n                history.push(\"/checkout\")\r\n            }\r\n            catch(err){\r\n                openNotificationWithIcon('error',err.response.statusText, err.response.data.msg);\r\n            }\r\n        }\r\n    }\r\n\r\n    const checkIfOutOfStock = () => {\r\n        let outOfStockItem =  cart.find(c => c.quantity === 0);\r\n        if(outOfStockItem) setDisable(true);\r\n    }\r\n\r\n    useEffect(() => {\r\n        checkIfOutOfStock();\r\n    }, [])\r\n\r\n    return (\r\n        <>\r\n        <StepWizard />\r\n        <div className=\"cart__wrapper\">\r\n            {cart && cart.length>0 ? \r\n            <><div className=\"cart__productDetails\">\r\n                <h3>My Cart {cart && cart.length>0 ? `(${totalCartProductsCount()})` : null}</h3>\r\n                {cart.map(c => {\r\n                    return <div key={c._id}>{productCart(c)}</div>;\r\n                })}\r\n                {user ? <div className=\"cart__placeorderWrapper\"><Button disabled={disable} className=\"cart__placeorder\" type=\"primary\" size=\"default\" onClick={checkoutHandler}>\r\n                        <span className=\"cart__placeorderText\">PROCEED TO CHECKOUT</span>\r\n                    </Button></div> : <Link to={{pathname:\"/login\", state: { source: \"/cart\" }}}><div className=\"cart__placeorderWrapper\"><Button disabled={disable} type=\"primary\" size=\"default\">Login to continue</Button></div></Link>}\r\n            </div>\r\n            <div className=\"cart__priceDetails\">\r\n                <h3>PRICE DETAILS</h3>\r\n                <div className=\"cart__priceBreakdown\">\r\n                    <div className=\"cart__priceLabel\">\r\n                        <p>Price ({cart.length} product{cart.length > 1 ? `s` : null})</p>\r\n                    </div>\r\n                </div>\r\n                {cart.map(c => {\r\n                    return <div className=\"cart__priceBreakdown\" key={c._id}>\r\n                        <div className=\"cart__priceLabel\">\r\n                            <p>{c.title.substring(0, 50)}... x <strong>{c.count}</strong></p>\r\n                        </div>\r\n                        <div className=\"cart__priceNum\">\r\n                            <p><CurrencyFormat value={c.price} displayType={'text'} thousandSeparator={true} prefix={'$'} renderText={value => <span>{value}</span>} /></p>\r\n                        </div>\r\n                    </div>\r\n                })}\r\n                <hr style={{width:'90%', margin:'0 auto', borderTop:'1px dashed #ccc'}} />\r\n                <div className=\"cart__priceBreakdown\">\r\n                    <div className=\"cart__priceLabel\">\r\n                        <p>Total Amount: </p>\r\n                    </div>\r\n                    <div className=\"cart__priceNum\">\r\n                        <p><strong><CurrencyFormat value={totalCartValue()} displayType={'text'} thousandSeparator={true} prefix={'$'} renderText={value => <span>{value}</span>} /></strong></p>\r\n                    </div>\r\n                </div>\r\n            </div></> : \r\n            <div className=\"cart__productDetails cart__empty\">\r\n                <div>\r\n                    <img src={emptyCart} />\r\n                    <p>Your cart is empty</p>\r\n                    <div className=\"cart__empty-shopnow\"><Link to=\"/shop\"><Button type=\"primary\" size=\"large\">Shop now</Button></Link></div>\r\n                </div>\r\n            </div> }\r\n        </div>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default Cart;","D:\\React\\techkart\\client\\src\\Reducers\\drawerReducer.js",[],"D:\\React\\techkart\\client\\src\\components\\CartDrawer\\CartDrawer.js",["278"],"import React from 'react';\r\nimport {useDispatch, useSelector} from 'react-redux';\r\nimport {Avatar, Drawer, Image, Tooltip, Button} from 'antd';\r\nimport {useHistory} from 'react-router-dom';\r\nimport noImage from '../../assets/images/No_Image.png';\r\n\r\nimport './CartDrawer.css';\r\n\r\nconst CartDrawer = () => {\r\n    const {user, cart, drawer} = useSelector(state => ({...state}));\r\n    const dispatch = useDispatch();\r\n    const history = useHistory();\r\n\r\n    const onCartDrawerClose = () => {\r\n        dispatch({\r\n            type: 'SHOW_HIDE_DRAWER',\r\n            payload: false\r\n        });\r\n    };\r\n\r\n    const goToCartHandler = () => {\r\n        dispatch({\r\n            type: 'SHOW_HIDE_DRAWER',\r\n            payload: false\r\n        });\r\n        history.push(\"/cart\");\r\n    }\r\n\r\n    return (\r\n        <Drawer\r\n            title=\"Your cart\"\r\n            width={window.innerWidth > 768 ? 300 : window.innerWidth - 150}\r\n            placement=\"right\"\r\n            closable={true}\r\n            onClose={onCartDrawerClose}\r\n            visible={drawer}\r\n        >\r\n            {cart && cart.length > 0 && cart.map(c => {\r\n                return <div className=\"cart__drawer-wrapper\" key={c._id}>\r\n                    <div className=\"cart__drawer-img\"><Avatar size=\"large\" shape=\"square\" src={<Image src={c.images.length > 0 ? c.images[0].url : noImage} />}/></div>\r\n                    <div className=\"cart__drawer-title\"><Tooltip title={c.title} placement=\"leftBottom\"><p>{c.title.substring(0,40)}...</p></Tooltip></div>\r\n                </div>\r\n            })}\r\n            <Button className=\"cart__placeorder\" type=\"primary\" size=\"default\" style={{width:'100%', marginTop:'10px'}} onClick={goToCartHandler}>\r\n                <span className=\"cart__placeorderText\">Go to cart</span>\r\n            </Button> \r\n        </Drawer>\r\n    )\r\n}\r\n\r\nexport default CartDrawer;\r\n","D:\\React\\techkart\\client\\src\\pages\\Checkout.js",["279"],"D:\\React\\techkart\\client\\src\\utils\\user-util.js",[],"D:\\React\\techkart\\client\\src\\components\\Forms\\AddressForm.js",["280"],"import React, { useEffect } from 'react';\r\nimport { Drawer, Form, Input, Button } from 'antd';\r\n\r\nconst AddressForm = ({visible, setVisible, form, address, setAddress, loading, addAddressHandler}) => {\r\n    \r\n    let jsonAddress = {name: '', mobile:'', pinCode:'', streetAdd:'', locality:'', City:'', State:''}; \r\n    if(address.deliveryAddress !== \"\" && address.deliveryAddress !== undefined)\r\n        jsonAddress = JSON.parse(address.deliveryAddress);\r\n\r\n    const showDrawer = () => {\r\n        if(address.mode !== \"edit\") setAddress({deliveryAddress:'', mode:''})\r\n        setVisible(true);\r\n    };\r\n\r\n    const onClose = () => {\r\n        setAddress({deliveryAddress:'', mode:''});\r\n        setVisible(false);\r\n    }\r\n\r\n    const onChangeHandler = (e) => {\r\n        setAddress({...address, deliveryAddress : JSON.stringify({...jsonAddress, [e.target.name]: e.target.value})});\r\n    }\r\n\r\n    useEffect(() => {\r\n        form.resetFields();\r\n    }, [address.mode]);\r\n\r\n    return (\r\n        <>\r\n            {address.mode !== \"edit\" && <Button type=\"primary\" className=\"checkout__addnewaddress\" size=\"default\" onClick={showDrawer}>ADD NEW ADDRESS</Button>}\r\n            <Drawer\r\n                title={address.mode !== \"edit\" ? \"Add Address\" : \"Edit Address\"}\r\n                width={window.innerWidth > 768 ? 500 : window.innerWidth - 75}\r\n                placement=\"right\"\r\n                closable={true}\r\n                onClose={onClose}\r\n                visible={visible}\r\n            >\r\n                <Form\r\n                    form={form}\r\n                    layout=\"vertical\"\r\n                    name={address.mode !== \"edit\" ? \"Add Address\" : \"Edit Address\"}\r\n                    scrollToFirstError\r\n                >\r\n                    <Form.Item\r\n                        label=\"Name\"\r\n                        name=\"name\"\r\n                        rules={[{ required: true, message: 'Please input name!' }]}\r\n                        initialValue={jsonAddress.name}\r\n                    >\r\n                        <Input name=\"name\" allowClear onChange={onChangeHandler} />\r\n                    </Form.Item>\r\n\r\n                    <Form.Item\r\n                        label=\"Mobile\"\r\n                        name=\"mobile\"\r\n                        rules={[{ required: true, message: 'Please input mobile number!' }]}\r\n                        initialValue={jsonAddress.mobile}\r\n                    >\r\n                        <Input name=\"mobile\" allowClear onChange={onChangeHandler} />\r\n                    </Form.Item>\r\n\r\n                    <Form.Item\r\n                        label=\"Pin Code\"\r\n                        name=\"pinCode\"\r\n                        rul